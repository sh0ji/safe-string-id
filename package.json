    "babelify": "^7.3.0",
    "browserify": "^14.0.0",
    "derequire": "^2.0.6",
    "eslint": "^4.15.0",
    "eslint-config-airbnb-base": "^12.1.0",
    "eslint-plugin-import": "^2.8.0",
    "uglifyify": "^3.0.4"
