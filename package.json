{
  "name": "web-id",
  "version": "1.1.6",
  "description": "Convert strings into web-usable ids.",
  "main": "dist/web-id.js",
  "scripts": {
    "build": "browserify index.js > dist/web-id.js -t [ babelify --presets [ es2015 ] ] -s WebId",
    "minify": "browserify -g uglifyify index.js > dist/web-id.min.js -t [ babelify --presets [ es2015 ] ] -s WebId",
    "prepublish": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sh0ji/safe-string-id.git"
  },
  "author": "Evan Yamanishi",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/sh0ji/safe-string-id/issues"
  },
  "homepage": "https://github.com/sh0ji/safe-string-id#readme",
  "devDependencies": {
    "babel-preset-es2015": "^6.22.0",
    "babelify": "^7.3.0",
    "browserify": "^14.0.0",
    "uglifyify": "^3.0.4"
  },
  "dependencies": {
    "lodash": "^4.17.4",
    "shortid": "^2.2.8"
  }
}
